{
  "name": "kaipk/contao-scrollmagic",
  "description": "Animate content elements on scrollspeed",
  "keywords": ["contao", "animate", "scroll", "viewport"],
  "type": "contao-bundle",
  "license": "MIT",
  "authors": [
	{
		"name": "KAIPO EDV IT Ges.m.b.H",
		"homepage": "https://github.com/kaipk/contao-scrollmagic"
	}  
  ],
  "require": {
    "php": ">=7.0",
    "contao/core-bundle": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "kaipk\\ScrollmagicBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "kaipk\\ScrollmagicBundle\\ContaoManager\\Plugin"
  }
}
